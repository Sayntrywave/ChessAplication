<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.vsu.korotkov.chess.fx.GameController">
   <children>
      <VBox alignment="CENTER" prefHeight="62.0" prefWidth="240.0">
         <children>
            <ToolBar prefHeight="283.0" prefWidth="512.0">
              <items>
                  <ToggleButton fx:id="localGameButton" mnemonicParsing="false" onAction="#onLocalGameButtonPressed" text="Local" />
                  <ToggleButton fx:id="remoteGameButton" mnemonicParsing="false" onAction="#onRemoteGameButtonPressed" text="Remote" />
              </items>
            </ToolBar>
            <AnchorPane fx:id="root" prefHeight="27.0" prefWidth="240.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
